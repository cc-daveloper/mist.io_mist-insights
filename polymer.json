{
  "entrypoint": "mist-insights.html",
  "sources": [
    "src/**/*",
    "data/**/*",
    "images/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "echarts-import.html",
    "bower_components/echarts/dist/echarts.min.js"
  ],
  "builds": [{
    "name": "bundled",
    "bundle": true,
    "js": {"minify": true},
    "css": {"minify": true},
    "html": {"minify": true}
  },{
    "name": "unbundled",
    "js": {"minify": true},
    "css": {"minify": true},
    "html": {"minify": true}
  }],
  "lint": {
    "rules": [
      "polymer-2-hybrid"
    ]
  }
}
